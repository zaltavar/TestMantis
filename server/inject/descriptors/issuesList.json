{
  "type": "object",
  "properties": {
    "myissuelist": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "issue": {
            "type": "string",
            "selector": "td.left",
            "control": "property.innerText"
          }
        }
      },
      "selectorAll": "tr[bgcolor]"
    },
    "isissueslist": {
      "type": "identifier",
      "control": "exists",
      "selectContains": "Assigned to Me (Unresolved)",
      "selector": ".subtle"
    }
  },
  "connect": [
    {
      "match": [
        "isissueslist"
      ],
      "screens": [
        "issuelist"
      ]
    }
  ],
  "selector": "table.width100",
  "selectContains": "Assigned to Me"
}
